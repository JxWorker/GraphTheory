<Window x:Class="GraphTheory.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GraphTheory"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="950">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <StackPanel Margin="0,0,750,0">
            <ComboBox Width="120" Margin="0,10,0,10">
                <ComboBoxItem Content="Create graph"/>
                <ComboBoxItem Content="AIT gragh"/>
                <ComboBoxItem Content="Test graph"/>
            </ComboBox>
            <Button Content="test" Click="testClick" ClickMode="Press"/>
            <Grid x:Name="selectContainer" Height="446"/>
        </StackPanel>
        <Grid x:Name="windowContainer" Margin="205,10,10,0"/>


    </Grid>
</Window>
