<Window x:Class="GraphTheory.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="960">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <StackPanel HorizontalAlignment="Left" Width="290">
            <ComboBox x:Name="graph_combobox" Width="120" Margin="0,10,0,10" SelectionChanged="ComboBox_SelectionChanged">
                <ComboBoxItem Content="Create graph"/>
                <ComboBoxItem Content="AIT gragh"/>
                <ComboBoxItem Content="Test graph"/>
            </ComboBox>
            <Grid x:Name="selectContainer_grid" Height="522" Width="290">
            </Grid>
        </StackPanel>
        <Grid x:Name="windowContainer_grid" HorizontalAlignment="Right" Width="660" Margin="0,10,0,0"/>


    </Grid>
</Window>
